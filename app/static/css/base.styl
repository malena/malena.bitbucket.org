p,
blockquote
	color: #A1A1A1

a 
	color: red 
	text-decoration: none

a:hover
	color: #5A5A5A

.social-icons
	text-align: center
	margin: 1em auto
	a 
		background-repeat: no-repeat;
		width: 28px
		height: 28px
		margin-right: 0.5em
		font-size: 30px 



h3.test
	font-size: 16px
	font-weight: normal
	transform-style: preserve-3d
	text-align: right;

.c1
	-moz-transform: scaleX(-1)
	-webkit-transform: scaleX(-1)
	-o-transform: scaleX(-1)
	transform: scaleX(-1)
	-ms-filter: fliph /*IE*/
	filter: fliph /*IE*/ 
	font-size: 8.5em
	display: inline-block
	vertical-align: bottom;
	line-height: 1.2em;

.c2
	font-size: 9em
.c3
	font-size: 7em
.c4
	font-size: 6em
.c5
	font-size: 5em
	font-size: 9em;
	font-weight: bold;
	line-height: 1.4em;
	vertical-align: bottom;
	padding-left: 5px;


.modal-dialog
	width: 250px	
	height: 250px

.modal-content
	border-radius: 140px 
	width: 100%
	height: 100%

.modal-body
	height: 100%
	text-align: center

	a
		text-align: center
		display: block

.modal-backdrop
	z-index: 2